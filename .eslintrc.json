{
  "extends": "airbnb",
  "parser": "babel-eslint",
  "env": {
    "node": true,
    "browser": true
  },
  "globals": {
    "React$Element": true,
    "ReactClass": true,
    "Action": true,
    "IncAction": true,
    "DecAction": true,
    "AppState": true,
    "CounterState": true
  },
  "rules": {
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }]
  }
}
